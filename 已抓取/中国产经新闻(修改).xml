<job jobClass="com.apabi.crawler.job.ParsePageURLNumberNameJob">		<PaperName>中国产经新闻</PaperName>		<SiteRoot>http://dzb.cien.com.cn/zgcjxw/</SiteRoot>		<IndexURLRegex><![CDATA[00000000/html/index.htm]]></IndexURLRegex>	    <PageRegex><![CDATA[<a href="([\s\S]{0,30}.htm)[\s\S]{0,5}class="bmml_con_div_name">第([\s\S]{0,20}?)版：([^<]*?)</a>]]></PageRegex>	    <PageImageRegex><![CDATA[<img class="bantu_img" src="([\s\S]{0,50}.jpg)" border="0" usemap="#Map"/>]]></PageImageRegex>		<ArticleRegex><![CDATA[<area shape="rect" coords="([^']*?)"[\s\S]{0,30}?href="([^>]+?.htm)">]]></ArticleRegex>		<ArticleImageRegex><![CDATA[<IMG align=center src="([\S]*)"]]></ArticleImageRegex>		<ArticleContentRegex><![CDATA[<div id="zoom">(.*?)</div>]]></ArticleContentRegex>		</job>